{
	"manifest_version": 3,
	"name": "taokasha",
	"version": "7.1.5",
	"description": "replace everyone with hu tao",
	"permissions": ["scripting"],
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://akasha.cv/*", "https://enka.network/*"],
			"js": ["index.js"],
			"run_at": "document_idle"
		}
	]
}
